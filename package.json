{
  "name": "craftcanvas",
  "productName": "CraftCanvaS Studio",
  "version": "1.1.0",
  "description": "A Minecraft Instance Manager and Image Studio",
  "author": "Eadullah Rahman Abid <mredadul@gmail.com>",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "setup": "node setup_libs.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.craftcanvas.app",
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "CraftCanvaS"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!node_modules/.cache/*",
      "!src/*"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.11.0",
    "@upscalerjs/default-model": "^1.0.0-beta.17",
    "@xenova/transformers": "^2.14.0",
    "axios": "^1.6.0",
    "fabric": "^5.3.0",
    "google-auth-library": "^10.5.0",
    "minecraft-launcher-core": "^3.18.0",
    "msmc": "^4.0.0",
    "upscaler": "^1.0.0-beta.19"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3"
  }
}
